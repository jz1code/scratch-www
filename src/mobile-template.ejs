<!DOCTYPE html>
<!--[if lt IE 9 ]> <html class="ie8"> <![endif]-->
<!--[if IE 9 ]> <html class="ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html>
<!--<![endif]-->

<head>
    <meta charset="UTF-8" />

    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=<%= htmlWebpackPlugin.options.viewportWidth %>, initial-scale=1">

    <!-- Prevent mobile Safari from making phone numbers -->
    <meta name="format-detection" content="telephone=no">

    <title>Scratch - <%= htmlWebpackPlugin.options.title %></title>


    <!-- Favicon & CSS normalize -->
    <link rel="shortcut icon" href="/favicon.ico" />
    <link rel="stylesheet" href="/css/lib/normalize.min.css" />

    <!-- Polyfills -->
    <script src="/js/polyfill.min.js"></script>
    <link rel="stylesheet" href="/css/keyboard.css" />
    <script src="https://cdn.bootcss.com/jquery/3.3.1/jquery.min.js"></script>
    <script src="/js/common.js"></script>
    <style type="text/css"> 
    .ttt-tile-image {
        border-radius: 1rem;
        background: #4d97ff;
        width: 100%;
    }
    .ttt-tile-image-img {
        display: none;
        border-radius: 1rem;
        width: 100%;
        z-index: 9000;
    }
    </style>
    <script>
        window.scratchConfig = {
            handleVmInitialized: (vm) => {
                window.vm = vm;
                console.log("VM初始化完毕");
            },

            handleGreenFlag: () => {
                // 关闭广告， 显示虚拟键盘
                document.getElementById('ad').style.display = 'none';
                document.getElementById('keyboard').style.display = 'block';
                console.log('关闭广告， 显示虚拟键盘')
            },
        }
    </script>
</head>

<body>
    <div id="app"></div>
    <script src="/js/common.bundle.js"></script>

    <!-- Scripts -->
    <script src="/js/mobile.intl.js"></script>
    <script src="/js/mobile.bundle.js"></script>

    <div class="ttt-tile-image" id="ad" style="display: block;">
        <img  class="ttt-tile-image-img" src='http://cdn2.jzcode.cn/assets/static/c214d7d769d9a0f3338b36aa31ac3a9a.jpg'></img>
    </div>

    <!-- 游戏键盘 -->
    <div class="game_keyboard" id="keyboard" style="display: none;">
        <div class="keyboard">
            <div class="switch">
                <div id="keyboardA">游戏</div>
                <!-- <div id="keyboardB">双人</div> -->
                <div id="keyboardC">数字</div>
                <!-- <div id="keyboardD">琴键</div>
              <div id="keyboardE">字母</div> -->
            </div>
            <div class="control_A flex">
                <div class="control_direct">
                    <div>
                        <p class="key button_up" id=""
                            style='background: url("/images/mobile/arrow.png") no-repeat left top;border: skyblue 3px solid;border-radius: 2rem;'>
                        </p>
                    </div>
                    <div>
                        <p class="key rotate_left button_left" id=""
                            style='background: url("/images/mobile/arrow.png") no-repeat left top;border: skyblue 3px solid;border-radius: 2rem;'>
                        </p>
                        <p class="key rotate_right button_right" id=""
                            style='background: url("/images/mobile/arrow.png") no-repeat left top;border: skyblue 3px solid;border-radius: 2rem;'>
                        </p>
                    </div>
                    <div>
                        <p class="key rotate_down button_down" id=""
                            style='background: url("/images/mobile/arrow.png") no-repeat left top;border: skyblue 3px solid;border-radius: 2rem;'>
                        </p>
                    </div>
                </div>
                <div class="control_space">
                    <p class="space button_space" id=""
                        style='background: url("/images/mobile/space.png") no-repeat left top;'></p>
                </div>
            </div>
            <div class="control_C flex">
                <div class="calculator">
                    <div class="key button_7" style='background: url("/images/mobile/7.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_8" style='background: url("/images/mobile/8.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_9" style='background: url("/images/mobile/9.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_4" style='background: url("/images/mobile/4.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_5" style='background: url("/images/mobile/5.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_6" style='background: url("/images/mobile/6.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_1" style='background: url("/images/mobile/1.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_2" style='background: url("/images/mobile/2.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_3" style='background: url("/images/mobile/3.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_c" style='background: url("/images/mobile/c.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_0" style='background: url("/images/mobile/0.png") no-repeat left top;' id="">
                    </div>
                    <div class="key button_d" style='background: url("/images/mobile/d.png") no-repeat left top;' id="">
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

</html>